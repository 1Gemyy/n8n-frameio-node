{
  "name": "@frameio-community/n8n-node",
  "version": "0.1.0",
  "description": "n8n community node for Frame.io API integration - video collaboration and asset management",
  "keywords": [
    "n8n-community-node-package",
    "frameio",
    "frame.io",
    "video",
    "collaboration",
    "asset management",
    "adobe"
  ],
  "license": "MIT",
  "homepage": "https://github.com/yourusername/n8n-nodes-frameio",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/n8n-nodes-frameio.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FrameIoApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/FrameIo/FrameIo.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "~1.2.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "n8n-core": "~1.0.0"
  }
}